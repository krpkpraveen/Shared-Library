pipeline {
    agent any
    stages {
        stage('Build') {
            steps {
                echo 'Building'
				script {
					if (12 > 3) {
						echo 'First If condition executed!'
					}
					
					if (12 > 33) {
						echo 'Second If condition executed!'
					}
					
					if (50 > 30) {
						echo 'Third If condition executed!'
					}
					
					
				}
            }
        }
        stage('Test') {
            steps {
                echo 'Testing'
            }
        }
        stage('Deploy') {
            steps {
                echo 'Deploying'
            }
        }
    }
}